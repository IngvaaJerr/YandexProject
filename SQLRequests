 # Задание 1 
 SELECT c."login", COUNT(o."inDelivery") AS countOrders 
 FROM "Couriers" AS c 
 LEFT JOIN "Orders" AS o ON o."courierId" = c."id" 
 WHERE o."inDelivery" = true 
 GROUP BY c."login";

# Задание 2

SELECT "track", CASE 
WHEN "finished" IS true THEN 2 
WHEN "cancelled" IS true THEN -1 
WHEN "inDelivery" IS true THEN 1 
ELSE 0 
END AS status 
FROM "Orders";
